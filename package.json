{
  "name": "vuejs-form-plugin",
  "version": "0.2.3",
  "description": "Simple wrapper for HTTP requests",
  "main": "dist/index.js",
  "devDependencies": {
    "axios": "^0.17",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^1.0.0",
    "expect": "^23.4.0",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "mocha-webpack": "^1.0.1",
    "moxios": "^0.4.0",
    "vue": "^2.5.13",
    "vue-loader": "^13.7.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.11.0"
  },
  "scripts": {
    "prod": "npm run production",
    "production": "NODE_ENV=production webpack --config=webpack.config.js",
