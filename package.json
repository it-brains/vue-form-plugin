  "devDependencies": {
    "axios": "^0.17",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^1.0.0",
    "expect": "^23.4.0",
